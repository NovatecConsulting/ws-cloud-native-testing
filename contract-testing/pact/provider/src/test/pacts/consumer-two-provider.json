{
  "consumer": {
    "name": "consumer-two"
  },
  "interactions": [
    {
      "description": "get single book",
      "providerStates": [
        {
          "name": "Getting book with ID [{id}] returns Clean Code",
          "params": {
            "id": "b3fc0be8-463e-4875-9629-67921a1e00f4"
          }
        }
      ],
      "request": {
        "method": "GET",
        "path": "/books/b3fc0be8-463e-4875-9629-67921a1e00f4"
      },
      "response": {
        "body": {
          "isbn": "9780132350884",
          "numberOfPages": 464,
          "title": "Clean Code"
        },
        "headers": {
          "Content-Type": "application/json"
        },
        "matchingRules": {
          "body": {
            "$.isbn": {
              "combine": "AND",
              "matchers": [
                {
                  "match": "type"
                }
              ]
            },
            "$.numberOfPages": {
              "combine": "AND",
              "matchers": [
                {
                  "match": "integer"
                }
              ]
            },
            "$.title": {
              "combine": "AND",
              "matchers": [
                {
                  "match": "type"
                }
              ]
            }
          }
        },
        "status": 200
      }
    }
  ],
  "metadata": {
    "pact-jvm": {
      "version": "4.2.5"
    },
    "pactSpecification": {
      "version": "3.0.0"
    }
  },
  "provider": {
    "name": "provider"
  }
}
